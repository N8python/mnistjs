{
  "name": "@thi.ng/math",
  "version": "5.11.9",
  "description": "Assorted common math functions & utilities",
  "type": "module",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/thi-ng/umbrella.git"
  },
  "homepage": "https://thi.ng/math",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/postspectacular"
    },
    {
      "type": "patreon",
      "url": "https://patreon.com/thing_umbrella"
    }
  ],
  "author": "Karsten Schmidt (https://thi.ng)",
  "contributors": [
    "@nkint (https://github.com/nkint)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn build:esbuild && yarn build:decl",
    "build:decl": "tsc --declaration --emitDeclarationOnly",
    "build:esbuild": "esbuild --format=esm --platform=neutral --target=es2022 --tsconfig=tsconfig.json --outdir=. src/**/*.ts",
    "clean": "bun ../../tools/src/clean-package.ts",
    "doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
    "doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
    "doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
    "pub": "yarn npm publish --access public",
    "test": "bun test",
    "tool:tangle": "../../node_modules/.bin/tangle src/**/*.ts"
  },
  "dependencies": {
    "@thi.ng/api": "^8.11.9"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.5",
    "esbuild": "^0.23.0",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "animation",
    "bezier",
    "cubic",
    "easing",
    "hermite",
    "interpolation",
    "interval",
    "math",
    "prime",
    "quadratic",
    "smoothmax",
    "smoothstep",
    "solver",
    "trigonometry",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "./*.js",
    "./*.d.ts"
  ],
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./abs": {
      "default": "./abs.js"
    },
    "./angle": {
      "default": "./angle.js"
    },
    "./api": {
      "default": "./api.js"
    },
    "./crossing": {
      "default": "./crossing.js"
    },
    "./easing": {
      "default": "./easing.js"
    },
    "./eqdelta": {
      "default": "./eqdelta.js"
    },
    "./extrema": {
      "default": "./extrema.js"
    },
    "./fit": {
      "default": "./fit.js"
    },
    "./int": {
      "default": "./int.js"
    },
    "./interval": {
      "default": "./interval.js"
    },
    "./libc": {
      "default": "./libc.js"
    },
    "./min-error": {
      "default": "./min-error.js"
    },
    "./mix": {
      "default": "./mix.js"
    },
    "./permutations": {
      "default": "./permutations.js"
    },
    "./prec": {
      "default": "./prec.js"
    },
    "./prime": {
      "default": "./prime.js"
    },
    "./ratio": {
      "default": "./ratio.js"
    },
    "./safe-div": {
      "default": "./safe-div.js"
    },
    "./solve": {
      "default": "./solve.js"
    },
    "./step": {
      "default": "./step.js"
    }
  },
  "thi.ng": {
    "year": 2013
  },
  "gitHead": "502a0192bae48e69290870ea8a2767f8b81adee6\n"
}